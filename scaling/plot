set terminal postscript
set xrange [0:24]
set xtics ("0:00" 0, "3:00" 3, "6:00" 6, "9:00" 9, "12:00" 12, "15:00" 15, "18:00" 18, "21:00" 21, "24:00" 24)
set xlabel "Time of day"
set ylabel "Fraction of 16:45-17:00 reference period"
plot "distrib2" using 1:2 with steps title "Vehicle scaling factor", "distrib2" using 1:3 with steps title "Pedestrian scaling factor"
